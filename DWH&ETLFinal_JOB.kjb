    <entry>
      <name>Create datamart table</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>CREATE TABLE IF NOT EXISTS datamart AS   
WITH ControlAverages AS (
    SELECT
        platename,
        AVG(CASE WHEN compoundname = 'n' THEN "Number of FUS GFP Spots Cyto Selected" END) AS avg_n_fus,
        AVG(CASE WHEN compoundname = 'p' THEN "Number of FUS GFP Spots Cyto Selected" END) AS avg_p_fus,
        AVG(CASE WHEN compoundname = 'n' THEN "Number of G3BP1 Spots Cyto Selected" END) AS avg_n_g3bp1,
        AVG(CASE WHEN compoundname = 'p' THEN "Number of G3BP1 Spots Cyto Selected" END) AS avg_p_g3bp1,
        AVG(CASE WHEN compoundname = 'n' THEN "Number of EIF3 Spots Cyto Selected" END) AS avg_n_eif3,
        AVG(CASE WHEN compoundname = 'p' THEN "Number of EIF3 Spots Cyto Selected" END) AS avg_p_eif3
    FROM
        zorkodashvili.compounds
    GROUP BY
        platename
)
SELECT
    cf.platename AS "plate_name",
    cf.compoundname AS "compound_name",
    cf.compoundconcentration AS "concentration",
    'Number of FUS GFP Spots Cyto Selected' AS "feature_name",
    100 * (cf."Number of FUS GFP Spots Cyto Selected" - ca.avg_n_fus) / (ca.avg_p_fus - ca.avg_n_fus) AS "percent_of_inhibition"
FROM
    zorkodashvili.compounds cf
JOIN
    ControlAverages ca ON cf.platename = ca.platename
WHERE
    cf.compoundname NOT IN ('p', 'n')

UNION ALL

SELECT
    cf.platename AS "plate_name",
    cf.compoundname AS "compound_name",
    cf.compoundconcentration AS "concentration",
    'Number of G3BP1 Spots Cyto Selected' AS "feature_name",
    100 * (cf."Number of G3BP1 Spots Cyto Selected" - ca.avg_n_g3bp1) / (ca.avg_p_g3bp1 - ca.avg_n_g3bp1) AS "percent_of_inhibition"
FROM
    zorkodashvili.compounds cf
JOIN
    ControlAverages ca ON cf.platename = ca.platename
WHERE
    cf.compoundname NOT IN ('p', 'n')

UNION ALL

SELECT
    cf.platename AS "plate_name",
    cf.compoundname AS "compound_name",
    cf.compoundconcentration AS "concentration",
    'Number of EIF3 Spots Cyto Selected' AS "feature_name",
    100 * (cf."Number of EIF3 Spots Cyto Selected" - ca.avg_n_eif3) / (ca.avg_p_eif3 - ca.avg_n_eif3) AS "percent_of_inhibition"
FROM
    zorkodashvili.compounds cf
JOIN
    ControlAverages ca ON cf.platename = ca.platename
WHERE
    cf.compoundname NOT IN ('p', 'n');</sql>
